{
  \"Version\": \"2012-10-17\",
  \"Statement\": [
    {
      \"Effect\": \"Allow\",
      \"Action\": [
        \"s3:PutObject\",
        \"s3:AbortMultipartUpload\",
        \"s3:ListBucket\",
        \"s3:GetObject\"
      ],
      \"Resource\": [
        \"arn:aws:s3:::my-export-bucket\",
        \"arn:aws:s3:::my-export-bucket/*\"
      ]
    },
    {
      \"Effect\": \"Allow\",
      \"Action\": [
        \"kms:Encrypt\",
        \"kms:Decrypt\",
        \"kms:GenerateDataKey\",
        \"kms:DescribeKey\"
      ],
      \"Resource\": \"*\"
    }
  ]
}
